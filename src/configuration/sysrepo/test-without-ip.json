{
  "ietf-detnet:detnet": {
    "app-flows": {
      "app-flow": [
        {
          "name": "app0",
	  "ingress": {
	    "interface": ["enp86s0.5"],
            "tsn-app-flow": {
              "destination-mac-address": "CB:CB:CB:CB:CB:CB",
              "vlan-id": 5,
              "pcp": 3
            }
	  }
        }
      ]
    },
    "service": {
      "sub-layer": [
        {
          "name": "ssl-1",
          "incoming": {
            "app-flow": {
              "flow": [
                "app0"
              ]
            }
          },
          "outgoing": {
            "forwarding-sub-layer": {
              "service-outgoing": [
                {
                  "index": 0,
                  "sub-layer": [
                    "fsl-1"
                  ]
                }
              ]
            }
          }
        }
      ]
    },
    "forwarding": {
      "sub-layer": [
        {
          "name": "fsl-1",
          "outgoing": {
            "interface": {
              "outgoing-interface": "ilan0"
            }
          }
        }
      ]
    }
  },
  "ietf-interfaces:interfaces": {
    "interface": [
      {
        "name": "enp86s0",
        "type": "iana-if-type:ethernetCsmacd"
      },
      {
        "name": "enp86s0.5",
        "type": "iana-if-type:l2vlan",
        "ietf-if-extensions:parent-interface": "enp86s0"
      },
      {
        "name": "ilan0",
        "type": "iana-if-type:ilan"
      }
    ]
  },
  "ieee802-dot1cb-stream-identification:stream-identity": [
    {
      "index": 0,
      "handle": 1,
      "in-facing": {
        "input-port": [
          "ilan0"
        ]
      },
      "null-stream-identification": {
        "destination-mac": "CB-CB-CB-CB-CB-CB",
        "vlan": 5
      }
    },
    {
      "index": 1,
      "handle": 1,
      "out-facing": {
        "output-port": [
          "enp86s0"
        ]
      },
      "dmac-vlan-stream-identification": {
        "down": {
          "priority": 3
        }
      }
    }
  ]
}

