{
  "ietf-detnet:detnet": {
    "traffic-profile": [
      {
        "name": "pf-1",
        "traffic-spec": {
          "interval": 2000000,
          "max-pkts-per-interval": 10,
          "max-payload-size": 1500
        }
      }
    ],
    "app-flows": {
      "app-flow": [
        {
          "name": "app0",
          "traffic-profile": "pf-1",
          "ingress": {
            "interface": "enp1s0.5",
            "ip-app-flow": {
              "src-ip-prefix": "192.168.2.1/32"
            }
          }
        }
      ]
    }
  },
  "ietf-interfaces:interfaces": {
    "interface": [
      {
        "name": "enp1s0",
        "type": "iana-if-type:ethernetCsmacd"
      },
      {
        "name": "enp1s0.5",
        "type": "iana-if-type:l2vlan",
        "ietf-if-extensions:parent-interface": "enp1s0"
      }
    ]
  },
  "tsn-interface-configuration:tsn-interface-configuration": {
    "interface-list": [
      {
        "mac-address": "AB-AB-AB-AB-AB-AB",
        "interface-name": "enp1s0.5",
        "config-list": [
          {
            "index": 0,
            "time-aware-offset": 0
          },
          {
            "index": 1,
            "ieee802-vlan-tag": {
              "priority-code-point": 3,
              "vlan-id": 5
            }
          },
          {
            "index": 2,
            "ieee802-mac-addresses": {
              "destination-mac-address": "CB-CB-CB-CB-CB-CB"
            }
          }
        ]
      }
    ]
  },
  "ieee1588-ptp:ptp": {
    "instances": {
      "instance": [
        {
          "instance-index": 1,
          "default-ds": {
            "clock-quality": {
              "clock-class": "ieee1588-ptp:cc-default",
              "clock-accuracy": "ieee1588-ptp:ca-time-accurate-to-gt-10-s",
              "offset-scaled-log-variance": 65535
            },
	    "domain-number": 0,
	    "sdo-id": 256
          },
          "time-properties-ds": {
            "current-utc-offset": 37,
            "current-utc-offset-valid": true,
            "leap59": false,
            "leap61": false,
            "time-traceable": true,
            "frequency-traceable": false,
            "ptp-timescale": true,
            "time-source": "ieee1588-ptp:internal-oscillator"
          }
        }
      ]
    }
  }
}

