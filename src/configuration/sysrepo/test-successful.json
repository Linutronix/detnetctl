{
  "ietf-detnet:detnet": {
    "traffic-profile": [
      {
        "name": "pf-1",
        "traffic-spec": {
          "interval": 2000000,
          "max-pkts-per-interval": 10,
          "max-payload-size": 1500
        }
      }
    ],
    "app-flows": {
      "app-flow": [
        {
          "name": "app0",
          "traffic-profile": "pf-1",
          "ingress": {
            "interface": ["enp86s0.5"]
          }
        }
      ]
    },
    "service": {
      "sub-layer": [
        {
          "name": "ssl-1",
          "incoming": {
            "app-flow": {
              "flow": [
                "app0"
              ]
            }
          },
          "outgoing": {
            "forwarding-sub-layer": {
              "service-outgoing": [
                {
                  "index": 0,
                  "sub-layer": [
                    "fsl-1"
                  ]
                }
              ]
            }
          }
        }
      ]
    },
    "forwarding": {
      "sub-layer": [
        {
          "name": "fsl-1",
          "outgoing": {
            "interface": {
              "outgoing-interface": "ilan0"
            }
          }
        }
      ]
    }
  },
  "ietf-interfaces:interfaces": {
    "interface": [
      {
        "name": "enp86s0",
        "type": "iana-if-type:ethernetCsmacd",
        "ieee802-dot1q-bridge:bridge-port": {
          "traffic-class": {
            "traffic-class-table": {
              "number-of-traffic-classes": 4
            }
          },
          "ieee802-dot1q-sched-bridge:gate-parameter-table": {
            "admin-control-list": {
              "gate-control-entry": [
                {
                  "index": 0,
                  "operation-name": "ieee802-dot1q-sched:set-gate-states",
                  "time-interval-value": 5000,
                  "gate-states-value": 1
                },
                {
                  "index": 1,
                  "operation-name": "ieee802-dot1q-sched:set-gate-states",
                  "time-interval-value": 5000,
                  "gate-states-value": 2
                }
              ]
            }
          }
        }
      },
      {
        "name": "enp86s0.5",
        "type": "iana-if-type:l2vlan",
        "ietf-if-extensions:parent-interface": "enp86s0",
        "ietf-ip:ipv4": {
          "address": [
            {
              "ip": "192.168.2.1",
              "prefix-length": 24
            }
          ]
        },
        "ietf-ip:ipv6": {
          "address": [
            {
              "ip": "fd2a:bc93:8476:634::",
              "prefix-length": 64
            }
          ]
        }
      },
      {
        "name": "ilan0",
        "type": "iana-if-type:ilan"
      }
    ]
  },
  "tsn-interface-configuration:tsn-interface-configuration": {
    "interface-list": [
      {
        "mac-address": "AB-AB-AB-AB-AB-AB",
        "interface-name": "enp86s0.5",
        "config-list": [
          {
            "index": 0,
            "time-aware-offset": 0
          },
          {
            "index": 1,
            "ieee802-vlan-tag": {
              "priority-code-point": 3,
              "vlan-id": 5
            }
          },
          {
            "index": 2,
            "ieee802-mac-addresses": {
              "destination-mac-address": "CB-CB-CB-CB-CB-CB"
            }
          }
        ]
      }
    ]
  },
  "ieee802-dot1cb-stream-identification:stream-identity": [
    {
      "index": 0,
      "handle": 1,
      "in-facing": {
        "input-port": [
          "ilan0"
        ]
      },
      "null-stream-identification": {
        "destination-mac": "CB-CB-CB-CB-CB-CB",
        "vlan": 5
      }
    },
    {
      "index": 1,
      "handle": 1,
      "out-facing": {
        "output-port": [
          "enp86s0"
        ]
      },
      "dmac-vlan-stream-identification": {
        "down": {
          "priority": 3
        }
      }
    }
  ],
  "ieee1588-ptp-tt:ptp": {
    "instances": {
      "instance": [
        {
          "instance-index": 1,
          "default-ds": {
            "clock-quality": {
              "clock-class": "ieee1588-ptp-tt:cc-default",
              "clock-accuracy": "ieee1588-ptp-tt:ca-time-accurate-to-gt-10-s",
              "offset-scaled-log-variance": 65535
            },
            "domain-number": 0,
            "sdo-id": 256
          },
          "time-properties-ds": {
            "current-utc-offset": 37,
            "current-utc-offset-valid": true,
            "leap59": false,
            "leap61": false,
            "time-traceable": true,
            "frequency-traceable": false,
            "ptp-timescale": true,
            "time-source": "ieee1588-ptp-tt:internal-oscillator"
          }
        }
      ]
    }
  }
}

